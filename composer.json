{
    "name": "memio/spec-gen",
    "license": "MIT",
    "type": "library",
    "description": "phpspec extension for better code generation",
    "keywords": ["phpspec", "extension", "generator", "PHP", "code"],
    "homepage": "http://memio.github.io/spec-gen",
    "authors": [
        {
            "name": "Lo√Øc Faugeron",
            "email": "faugeron.loic@gmail.com",
            "homepage": "http://gnugat.github.io",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Memio\\SpecGen\\": "src/Memio/SpecGen"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Memio\\SpecGen\\Fixtures\\": "fixtures",
            "tests\\Memio\\SpecGen\\": "tests"
        }
    },
    "repositories": [
        {"type": "vcs", "url": "https://github.com/webhdx/memio.git"},
        {"type": "vcs", "url": "https://github.com/webhdx/twig-template-engine.git"}
    ],
    "require": {
        "gnugat/redaktilo": "^2.0",
        "memio/memio": "dev-object_issue as 2.0.x-dev",
        "memio/linter": "^2.0",
        "memio/validator": "^2.0",
        "memio/model": "^2.0.1",
        "memio/pretty-printer": "^2.0",
        "memio/twig-template-engine": "dev-twig3 as 2.0.x-dev",
        "php": "^7.2",
        "phpspec/phpspec": "^6.1",
        "symfony/event-dispatcher": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.4"
    }
}
